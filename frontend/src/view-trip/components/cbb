



import React from 'react';
import { IoTicketOutline } from "react-icons/io5";

const Place = ({ trip }) => {
  // Extract itinerary from the trip data hierarchy
  const itinerary = trip?.tripdata?.[0]?.itinerary;

  // If no itinerary is found, display a message
  if (!itinerary || !Array.isArray(itinerary)) {
    return <p className="text-gray-600">No itinerary found.</p>;
  }

  return (
    <div className="mt-12">
      <h1 className="text-xl font-bold mb-5">Places to Visit</h1>
      {itinerary.map((dayPlan, index) => (
        <div key={index} className="mb-8">
          {/* Day Heading */}
          <h1 className="text-xl font-bold mb-2 mt-8">Day {dayPlan.day}</h1>

          {dayPlan.
            placesToVisit
            .map((place, placeIndex) => (
              <div
                key={placeIndex}
              >
                <h2 className="text-lg font-semibold mb-3 mt-4 text-orange-300">
                  Best Time to Visit - {place.bestTimeToVisit}
                </h2>
                <div className="relative border rounded-lg shadow-sm overflow-hidden transform transition-transform duration-300 hover:scale-105 hover:shadow-lg">

                  <div className="mt-6 grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 xl:grid-cols-4 gap-5 ">

                    {place.imageURL && (
                      <img
                        className="h-[150px] w-full object-cover rounded-t-lg"
                        src="data:image/jpeg;base64,"
                        alt="data:image/jpeg;base64,"
                      />
                    )}

                    {/* Place Details */}
                    <div className="p-4 bg-white">
                      <h3 className="text-xl font-bold">{place.placeName}</h3>
                      <p className="mt-2 text-gray-500">{place.details}</p>
                      <p className="mt-2 text-gray-600">
                        🕒 Best Time to Visit: {place.bestTimeToVisit}
                      </p>
                      <p className="mt-2 text-gray-600">
                        🚗 Travel Time: {place.travelTimeFromPrevious}
                      </p>
                      <p className="text-gray-500 mt-2">
                        <span className="font-bold">
                          <img
                            className="h-[30px] inline-block"
                            src="/ticket.png"
                            alt={<IoTicketOutline />}
                          />
                        </span>{" "}
                        {place.ticketPricing}
                      </p>
                    </div>

                    {/* Hover Overlay */}
                    <div className="absolute inset-0 bg-blue-300 bg-opacity-50 opacity-0 hover:opacity-100 transition-opacity duration-300 flex items-center justify-center">
                      <p className="text-white text-lg font-semibold">View Details</p>
                    </div>
                  </div>
                </div>
              </div>
            ))}
        </div>
      ))}
    </div>
  );
};

export default Place;





